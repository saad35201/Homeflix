<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CornerCut" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/_10sdp</item>
    </style>

    <style name="et_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:paddingStart">@dimen/_10sdp</item>
        <item name="android:layout_marginStart">@dimen/_15sdp</item>
        <item name="android:layout_marginEnd">@dimen/_15sdp</item>
        <item name="android:paddingEnd">@dimen/_10sdp</item>
        <item name="android:layout_marginTop">@dimen/_15sdp</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:importantForAccessibility">no</item>
        <item name="android:background">@drawable/primary_grey_round_corners_bg</item>
    </style>

    <style name="btn_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:layout_marginStart">@dimen/_15sdp</item>
        <item name="android:layout_marginEnd">@dimen/_15sdp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@drawable/primary_grey_round_corners_bg</item>
    </style>

</resources>